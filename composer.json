{
  "name": "dragoonboots/a2b",
  "description": "PHP data migration/transformation library",
  "type": "symfony-bundle",
  "homepage": "https://dragoonboots.github.io/A2B/",
  "readme": "https://github.com/DragoonBoots/A2B/blob/master/README.md",
  "support": {
    "issues": "https://github.com/DragoonBoots/A2B/issues",
    "source": "https://github.com/DragoonBoots/A2B",
    "docs": "https://dragoonboots.github.io/A2B/"
  },
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Dan Keenan",
      "email": "dk@dankeenan.org"
    }
  ],
  "require": {
    "doctrine/annotations": "^1.6",
    "doctrine/collections": "^1.5",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/inflector": "^2.0",
    "doctrine/orm": "^2.6",
    "ext-json": "*",
    "league/csv": "^9.1",
    "marcj/topsort": "^1.1",
    "php": "^7.3|^8.0",
    "php-ds/php-ds": "^1.3",
    "symfony/console": "^4.1|^5.0",
    "symfony/doctrine-bridge": "<=5.2.1",
    "symfony/finder": "^4.1|^5.0",
    "symfony/framework-bundle": "^4.1|^5.0",
    "symfony/property-access": "^4.1|^5.0",
    "symfony/proxy-manager-bridge": "^4.1|^5.0",
    "symfony/var-dumper": "^4.1|^5.0",
    "symfony/yaml": "^4.1|^5.0"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "symfony/maker-bundle": "^1.5",
    "symfony/phpunit-bridge": "^4.1|^5.0"
  },
  "suggest": {
    "ext-ds": "Can greatly increase performance",
    "symfony/maker-bundle": "Generate migrations from the command line."
  },
  "autoload": {
    "psr-4": {
      "DragoonBoots\\A2B\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DragoonBoots\\A2B\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "build-doc": [
      "sphinx-build -n -W --keep-going -b html doc doc/_build"
    ]
  }
}
